{
    "name": "thefair/hyperf-rocketmq",
    "type": "library",
    "description": "The PHP library for hyperf rocketmq",
    "keywords": [
        "rocketmq"
    ],
    "homepage": "https://github.com/thefair-net/hyperf_rocketmq",
    "license": "MIT",
    "authors": [
        {
            "name": "Zhang Han",
            "email": "zhanghan@thefair.net.cn"
        },
        {
            "name": "Liu mingzhi",
            "email": "liumingzhi@thefair.net.cn"
        }
    ],
    "require": {
        "php": ">=7.2",
        "hyperf/guzzle": "~2.0.0"
    },
    "require-dev": {
        "swoole/ide-helper": "^4.5",
        "phpmd/phpmd": "^2.6",
        "friendsofphp/php-cs-fixer": "^2.14",
        "mockery/mockery": "^1.0",
        "doctrine/common": "^2.9",
        "phpstan/phpstan": "^0.12"
    },
    "autoload": {
        "psr-4": {
            "TheFairLib\\RocketMQ\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        },
        "hyperf": {
            "config": "TheFairLib\\RocketMQ\\ConfigProvider"
        }
    }
}
